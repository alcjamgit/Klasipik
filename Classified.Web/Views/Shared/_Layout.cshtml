<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/cssBundle")
    @RenderSection("styles", false)
    @*<link href='http://fonts.googleapis.com/css?family=Roboto' rel='stylesheet' type='text/css'>*@
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>

    <div class="navbar navbar-default navbar-fixed-top">
        <div class="container">
            <div class="navbar-header" style="z-index:10005">
                <div class="pull-left">
                    <button type="button" class="navbar-toggle" style="margin-right:0; margin-left: 10px"  data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                </div class="pull-left">
                @Html.ActionLink("K", "Index", "Home", new { area = "" }, new { @class = "navbar-brand hidden-md hidden-lg hidden-sm" })
                @Html.ActionLink("Klasimo", "Index", "Home", new { area = "" }, new { @class = "navbar-brand hidden-xs" })
                <button class="navbar-btn pull-left btn btn-primary">
                    <span class="glyphicon glyphicon-plus aria-hidden="true"></span>
                <span class="hidden-xs">Post Ad</span></button>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                   <li>@Html.ActionLink("Categories", "Index", "Home", new { area = "" }, new { @class = "" })</li> 
                </ul>
                @Html.Partial("_LoginPartial")
                <div class="nav">
                    <div class="devsite-search-wrapper devsite-history-enabled">
                        <form class="devsite-search-form" action="" method="GET" id="top-search">
                            <div id="searchbox" class="devsite-searchbox">
                                <input placeholder="Search" type="text" class="devsite-search-field devsite-search-query hidden-sm" name="q" value="" autocomplete="off">
                            </div>
                            <div class="devsite-search-image">
                                <i class="glyphicon glyphicon-search add-margin-top" style="color:#fff"></i>
                            </div>
                            <div class="devsite-popout devsite-popout-closed">
